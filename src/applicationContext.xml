<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="firstComponent" class="com.example.SpringDoNotFail.ClassOne">
        <constructor-arg ref="secondComponent"/>
    </bean>

    <bean id="secondComponent" class="com.example.SpringDoNotFail.ClassTwo"/>

    <bean id="myComponent" class="com.example.SpringDoNotFail.ClassThree">

    </bean>
</beans>